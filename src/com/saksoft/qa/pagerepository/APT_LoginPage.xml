<?xml version="1.0" encoding="UTF-8"?>

<locators>

	<APT_headerlogin>
		<Username>name=j_username</Username>
		<Password>name=j_password</Password>
		<Loginbutton>//input[@class='formButton']</Loginbutton>
	</APT_headerlogin>
	
	<APT_invalidHeaderLogin>
		<Username>name=j_username</Username>
		<Password>name=j_password</Password>
		<Loginbutton>//input[@class='formButton']</Loginbutton>
	</APT_invalidHeaderLogin>
	
    <APT_login_1>
		<Username>//input[@placeholder='Username']</Username>
		<Password>//input[@placeholder='Password']</Password>
		<Loginbutton>//button[text()='Login']</Loginbutton>
	</APT_login_1>
	
	


	<APT_login>
		<Username>name=j_username</Username>
		<Password>name=j_password</Password>
		<Loginbutton>//input[@type='submit' and @class='buttons']</Loginbutton>
	</APT_login>
	

	<APT_invalidLogin>
		<Username>name=j_username</Username>
		<Password>name=j_password</Password>
		<Loginbutton>//input[@type='submit' and @class='buttons']</Loginbutton>
	</APT_invalidLogin>


	<APT_reset>
		<resetButton>//input[@type='reset']</resetButton>
    </APT_reset>


	<APT_forgotpassword>
		<resetButton>//a[contains(text(),'Forgot your Password?')]</resetButton>
	</APT_forgotpassword>
	
	<APT_logout>
		<resetButton>//a[contains(text(),'Forgot your Password?')]</resetButton>
	</APT_logout>
	
	
	<APT_editprofile>
	    <FirstName>name=user.givenName</FirstName>
	    <Surname>name=user.lastName</Surname>
	    <Password>name=user.password</Password>
	    <ConfirmPassword>name=confirmPassword</ConfirmPassword>
	    <PostalAddress>name=user.postalAddress</PostalAddress>
	    <Phone>name=user.telephoneNumber</Phone>
	    <Email>name=user.email</Email>
	    <OkButton>//input[@type='submit' and @value='OK']</OkButton>
	    <CancelButton>//input[@type='submit' and @value='Cancel']</CancelButton>
	</APT_editprofile>


</locators>
